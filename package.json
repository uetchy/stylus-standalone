{
  "name": "stylus-standalone",
  "description": "Standalone stylus transpiler.",
  "version": "0.1.0",
  "author": "Yasuaki Uechi <y@uechi.io> (https://uechi.io/)",
  "scripts": {
    "build": "rollup -c",
    "clean": "shx rm -rf lib",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "release-it"
  },
  "main": "lib/stylus-standalone.js",
  "browser": "lib/stylus-standalone.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "cross-fetch": "^3.0.5",
    "stylus": "^0.54.8"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime-corejs3": "^7.11.2",
    "@release-it/conventional-changelog": "^1.1.4",
    "release-it": "^13.7.0",
    "rollup": "^2.26.5",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-strip": "^1.2.2",
    "rollup-plugin-terser": "^7.0.0",
    "shx": "^0.3.2"
  },
  "homepage": "https://github.com/uetchy/stylus-standalone",
  "repository": {
    "type": "git",
    "url": "https://github.com/uetchy/stylus-standalone.git"
  },
  "bugs": {
    "url": "https://github.com/uetchy/stylus-standalone/issues"
